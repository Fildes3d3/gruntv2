<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
        <meta name="theme-color" content="#202020">
        <meta name="description" content="The Daily Grunt">
        <link rel="profile" href="http://gmpg.org/xfn/11">
        <title>{% block title %}The Daily Grunt{% endblock %}</title>

        {% block stylesheets %}
            <link rel='stylesheet' id='contact-form-7-css'  href='{{ asset("newTheme/content/plugins/contact-form/css/styles.css") }}' type='text/css' media='all' />
            <link rel='stylesheet' id='tithonia-style-css'  href='{{ asset("newTheme/content/themes/style.css") }}' type='text/css' media='all' />
            <link rel='stylesheet' id='font-awesome-css'  href='{{ asset("newTheme/content/themes/css/font-awesome.min.css") }}' type='text/css' media='all' />
            <link rel='stylesheet' id='bxslider-css'  href='{{ asset("newTheme/content/themes/css/jquery.bxslider.css") }}' type='text/css' media='all' />
            <link rel='stylesheet' id='tithonia-responsive-css'  href='{{ asset("newTheme/content/themes/css/responsive.css") }}' type='text/css' media='all' />
            <link rel="stylesheet" href="{{ asset('css/lightbox.css') }}"/>
            <link rel='stylesheet' id='tithonia-fonts-css'  href='https://fonts.googleapis.com/css?family=Droid+Serif%3A400%2C400italic%2C700%2C700italic&#038;subset=latin' type='text/css' media='all' />

        {% endblock %}
        <script type='text/javascript' src='{{ asset("newTheme/includes/js/jquery/jquery.js") }}'></script>
        <script type='text/javascript' src='{{ asset("newTheme/includes/js/jquery/jquery-migrate.min.js") }}'></script>


    </head>

        {% block body %}{% endblock %}

        {% block javascripts %}
            <script src="{{ asset('js/jquery-3.1.1.min.js') }}"></script>
            <script src="{{ asset('js/lightbox-plus-jquery.min.js') }}"></script>
            <script src="{{ asset('js/imagelightbox.min.js') }}"></script>
       <script>

           lightbox.option({
               'disableScrolling' : true,
               'resizeDuration': 200,
               'wrapAround': true,
               'fitImagesInViewport' : true
           });

       </script>

        <script type='text/javascript' src='{{ asset("newTheme/content/plugins/contact-form/js/jquery.form.min.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/plugins/contact-form/js/scripts.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/themes/js/slicknav.min.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/themes/js/jquery.bxslider.min.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/themes/js/jquery.fitvids.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/themes/js/tit.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/includes/js/wp-embed.min.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/includes/js/comment-reply.js") }}'>
        </script>
        <script type='text/javascript' src='{{ asset("newTheme/content/plugins/mailchimp/forms-api.min.js") }}'>
        </script>
        {% endblock %}
    </body>
</html>
